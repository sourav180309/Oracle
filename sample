DECLARE ---276 sec with for loop for 3.6million data with for loop
TYPE t_name IS TABLE OF src_objects%ROWTYPE
INDEX BY PLS_INTEGER;
V_ind t_name;
CURSOR c1 IS 
SELECT * FROM src_objects ;
BEGIN 
OPEN c1 ;
LOOP
FETCH c1 BULK COLLECT INTO v_ind LIMIT 1000000;
FOR i IN 1..v_ind.COUNT LOOP 
INSERT INTO tgt_objects
(object_name, 
subobject_name, 
object_id, 
data_object_id, 
object_type, 
created, 
last_ddl_time, 
timestamp, 
status, 
temporary, 
generated, 
secondary)
VALUES (
v_ind(i).object_name, 
v_ind(i).subobject_name, 
v_ind(i).object_id, 
v_ind(i).data_object_id, 
v_ind(i).object_type, 
v_ind(i).created, 
v_ind(i).last_ddl_time, 
v_ind(i).timestamp, 
v_ind(i).status, 
v_ind(i).temporary, 
v_ind(i).generated, 
v_ind(i).secondary
);
END LOOP;
EXIT WHEN v_ind.COUNT=0;
COMMIT;
END LOOP;
CLOSE c1;
END;

DECLARE --78 sec with index with forall , 53 without indexes for 3.6 million data
TYPE t_name IS TABLE OF src_objects%ROWTYPE
INDEX BY PLS_INTEGER;
V_ind t_name;
CURSOR c1 IS 
SELECT * FROM src_objects ;
BEGIN 
OPEN c1 ;
LOOP
FETCH c1 BULK COLLECT INTO v_ind LIMIT 1000000;
FORALL i IN v_ind.FIRST..v_ind.LAST 
INSERT INTO tgt_objects
VALUES v_ind(I);
EXIT WHEN v_ind.COUNT=0;
END LOOP;
CLOSE c1;
COMMIT;
END;



DECLARE --78 sec with index with forall , 53 without indexes for 3.6 million data

TYPE t_name IS TABLE OF src_objects%ROWTYPE

INDEX BY PLS_INTEGER;

V_ind t_name;

c1 sys_refcursor;

BEGIN 

OPEN c1 for select * from src_objects ;

LOOP

FETCH c1 BULK COLLECT INTO v_ind LIMIT 1000000;

FORALL i IN v_ind.FIRST..v_ind.LAST 

INSERT INTO tgt_objects

VALUES v_ind(I);

EXIT WHEN v_ind.COUNT=0;

END LOOP;

CLOSE c1;

COMMIT;

END;



